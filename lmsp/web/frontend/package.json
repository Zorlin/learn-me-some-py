{
  "name": "lmsp-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:all": "concurrently -n \"vite,api\" -c \"cyan,magenta\" \"vite\" \"cd ../../.. && uvicorn lmsp.web.app:app --reload --host 0.0.0.0 --port 8000\"",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
